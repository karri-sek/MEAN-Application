.container
    .table-responsive
    table.table.table-hover.table-striped.table-condensed
        thead
            tr
                th id
                th Supplier
                th Product
                th Price
            tbody
                tr(ng-repeat="product in products")
                    td {{product.id}}
                    td {{product.supplier}}
                    td {{product.product}}
                    td {{product.price}}